.push {
  list-style: none;
  font-family: 'Pushster', cursive;
}
.push li {
  position: relative;
  padding: 20px 0 20px 40px;
  color: red;
  font-variant: small-caps;
  font-weight: bold;
  cursor: pointer;
  font-size: 25px;
}
.push li:hover{
  position: relative;
  padding: 20px 0 20px 40px;
  color: red;
  font-variant: small-caps;
  font-weight: bold;
  cursor: pointer;
  padding-left: 25px;
  text-shadow: 17px 17px 21px rgba(145, 150, 150, 1);
}
.push li:before {
  position: absolute;
  width: 7px;
  height: 7px;
  border-radius: 50%;
  background: #4F5151;
  content: "";
  left: 0;
  transition: .3s ease-in-out;
  top: 27px;
}
.push li:after {
  position: absolute;
  border-left: 1px dotted #4F5151;
  width: 1px;
  bottom: -12px;
  content: "";
  left: 3px;
  top: 48px;
}
.push li:hover:before{box-shadow: 0 0 0 10px rgba(0,0,0,.2)}
.push li:last-child:after {content: none;}
