}

@keyframes slidein {
  0%{
transform: translate3d(-2000px, 40px, 0px);
 width: 50%;
 opacity: 1;
 z-index: 1;
  }

  100%{
transform: translate3d(2000px, 40px, 0px);
    width: 50%;
	opacity: 1;
	z-index: 1;
  }
}